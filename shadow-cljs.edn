{:source-paths ["src"]

 :dependencies [[re-frame "1.3.0-rc2" :exclusions [reagent]]
                [reagent "1.1.0" :exclusions [cljsjs/react cljsjs/react-dom]]]

 :nrepl {:middleware [refactor-nrepl.middleware/wrap-refactor]}

